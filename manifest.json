{
    "name": "Timetable SGU",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "This extension will crate timetable suitable for you",
    "homepage_url": "http://extensionizr.com",
    "icons": {
        "16": "icons/icon128.png",
        "48": "icons/icon128.png",
        "128": "icons/icon128.png"
    },
    "default_locale": "en",
    "browser_action": {
        "default_icon": "icons/icon128.png",
        "default_title": "browser action demo",
        "default_popup": "src/browser_action/browser_action.html"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],


    "permissions": [
        "tabs",
        "storage"
    ]



}